msgid "Maximum requests per minute per user"
msgstr "प्रति उपयोगकर्ता प्रति मिनट अधिकतम अनुरोध"

#: includes/class-api-proxy.php:45
msgid "Invalid nonce"
msgstr "अमान्य nonce"

#: includes/class-api-proxy.php:51
msgid "Invalid request"
msgstr "अमान्य अनुरोध"

#: includes/class-api-proxy.php:58
msgid "Text selection is required"
msgstr "टेक्स्ट चयन आवश्यक है"

#: includes/class-api-proxy.php:64
msgid "Text selection is too short"
msgstr "टेक्स्ट चयन बहुत छोटा है"

